<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:text="RV"
            android:layout_width="wrap_content"
            android:layout_height="14dp"
            android:id="@+id/main_land_tv_rv" android:layout_marginTop="36dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="84dp"/>
    <TextView
            android:text="0"
            android:layout_width="21dp"
            android:layout_height="14dp"
            android:id="@+id/main_land_tv_rv_result"
            app:layout_constraintStart_toEndOf="@+id/main_land_tv_rv"
            android:layout_marginStart="72dp" android:layout_marginTop="36dp"
            app:layout_constraintTop_toTopOf="parent" android:textAlignment="textEnd"/>
    <TextView
            android:text="0"
            android:layout_width="24dp"
            android:layout_height="16dp"
            android:id="@+id/main_land_tv_rh_result"
            app:layout_constraintStart_toEndOf="@+id/main_land_tv_rh"
            android:layout_marginStart="68dp" app:layout_constraintTop_toBottomOf="@+id/main_land_tv_rv_result"
            android:layout_marginTop="36dp" android:textAlignment="textEnd"/>
    <TextView
            android:text="0"
            android:layout_width="24dp"
            android:layout_height="17dp"
            android:id="@+id/main_land_tv_inches_result"
            app:layout_constraintStart_toEndOf="@+id/main_land_tv_inches"
            android:layout_marginStart="48dp" app:layout_constraintTop_toBottomOf="@+id/main_land_tv_rh_result"
            android:layout_marginTop="32dp" android:textAlignment="textEnd"/>
    <TextView
            android:text="0"
            android:layout_width="41dp"
            android:layout_height="16dp"
            android:id="@+id/main_land_tv_dpi_result"
            app:layout_constraintStart_toEndOf="@+id/main_land_tv_dpi"
            android:layout_marginStart="68dp" app:layout_constraintTop_toBottomOf="@+id/main_land_tv_inches_result"
            android:layout_marginTop="28dp" android:textAlignment="textEnd"/>
    <TextView
            android:text="RH"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:id="@+id/main_land_tv_rh" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/main_land_tv_rv" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="84dp"/>
    <TextView
            android:text="inches"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:id="@+id/main_land_tv_inches" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/main_land_tv_rh" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="84dp"/>
    <TextView
            android:text="dpi"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:id="@+id/main_land_tv_dpi" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/main_land_tv_inches" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="84dp"/>
</android.support.constraint.ConstraintLayout>