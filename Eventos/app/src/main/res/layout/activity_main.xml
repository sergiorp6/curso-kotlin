<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".presentationLayer.controllers.activities.MainActivity">
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/main_btAsistencia" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="128dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/main_etDorsal"
            android:text="Iniciar Actividad"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/main_btParar" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="128dp"
            android:layout_marginTop="28dp" app:layout_constraintTop_toBottomOf="@+id/main_btAsistencia"
            android:text="Parar Actividad"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/main_etDorsal" android:layout_marginTop="264dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="96dp"
            app:layout_constraintStart_toStartOf="parent"/>
</android.support.constraint.ConstraintLayout>