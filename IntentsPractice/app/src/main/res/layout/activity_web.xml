<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".presentationlayer.controllers.activities.WebActivity">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/web_et_web" android:layout_marginTop="132dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="96dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="Navegar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/web_bt_navegar"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="160dp" android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/web_et_web"/>
    <Button
            android:text="Buscar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/web_bt_buscar" android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/web_bt_navegar" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="160dp"/>
</android.support.constraint.ConstraintLayout>